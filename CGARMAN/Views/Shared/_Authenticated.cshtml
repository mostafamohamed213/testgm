        function ToggleColorInDashboard(id) {
            let IdsItems = ['linkAllVehicleStatus', 'linkAllTireSizes', 'linkCreateTireSize', 'linkIndexTechniciansDailyAttendance', 'linkTechniciansDays',
                'linkAllCostCenters', 'linkCreateCostCenter', 'linkCreatePosition', 'linkIndexTechniciansPosition', 'linkIndexTechniciansShift',
                'linkCreateShift', 'linkIndexTechniciansCompanies', 'linkCreateCompany', 'linkAllTechnicians', 'ItemsScreen', 'linkCreateTechnicians',
                'CreateItemScreen', 'CreateItemNameScreen', 'ItemNameScreen', 'StatusManagement', 'UnitManagement', 'VendorManagement',
                'ClassificationsManagement', 'BrandManagement', 'ModelManagement', 'linkAllVehicleDepartments','linkAllVehicleOwners'];
            IdsItems.forEach(function (i) {
                if (i == id) {
                    document.getElementById(i).classList.add('active');
                } else {
                    var item = document.getElementById(i);
                    if (item != null) {
                        item.classList.remove('active');
                    }
                }
            });
        }
        function throwException(response) {
            $("#ChangingBody").empty();
            $('#ChangingBody').append('<div class="container-fluid"><div class="text-center"><div class="error mx-auto" data-text="Error">Error</div><p class="lead text-gray-800 mb-5"><span>State</span> :' + response.readyState + '</p><a href="/Home/Index">&larr; Back to Home</a></div></div>');
        }
        function getspinner() {

            $("#ChangingBody").empty();
            $('#ChangingBody').append('<div style=" margin: 20% 50%;" class="spinner-grow text-primary"></div>');

